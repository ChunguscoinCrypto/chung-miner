{
  "name": "chung-miner",
  "productName": "chung-miner",
  "version": "1.0.0",
  "description": "Easy to use Chunguscoin Miner",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": {
    "name": "alamshafil",
    "email": "36016500+alamshafil@users.noreply.github.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "make_targets": {
        "win32": ["squirrel"],
        "darwin": ["zip"],
        "linux": ["zip"]
      },
      "github_repository": {
        "owner": "ChunguscoinCrypto",
        "name": "chung-miner"
      },
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "ChunguscoinCrypto",
              "name": "chung-miner"
            },
            "prerelease": true
          }
        }
      ],
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "chung_miner"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin",
            "linux"
          ]
        }
      ]
    }
  },
  "dependencies": {
    "chart.js": "^3.3.0",
    "electron-squirrel-startup": "^1.0.0",
    "node-os-utils": "^1.3.5",
    "systeminformation": "^5.7.4"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.55",
    "@electron-forge/maker-deb": "^6.0.0-beta.55",
    "@electron-forge/maker-rpm": "^6.0.0-beta.55",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.55",
    "@electron-forge/maker-zip": "^6.0.0-beta.55",
    "electron": "12.0.9"
  }
}
